<!DOCTYPE html>
<html>
<head>
  <title>Зачётная работа</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= link_to "Вход", '/session/auth' %> | <%= link_to "Главная страница" , '/place/main' %> | <%= link_to "Выход" , '/session/logout' if session[:current_user_id] %> </br>
<%= "Пользователь:  #{User.find_by_id(session[:current_user_id]).login}" if session[:current_user_id] %>
<%= 'Неавторизованный пользователь' if !session[:current_user_id] %>

<%= yield %>

</body>
</html>
